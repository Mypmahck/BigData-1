  </servlet>
	
	<servlet-mapping>
		<servlet-name>dispatcher</servlet-name>
		<url-pattern>/</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>CatDataServlet</servlet-name>
		<url-pattern>/catdata</url-pattern>
	</servlet-mapping>
